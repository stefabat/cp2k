&GLOBAL
  PROJECT  CH3_md_admm
  PRINT_LEVEL MEDIUM
  RUN_TYPE MD
  &TIMINGS
     THRESHOLD 0.01
  &END
&END GLOBAL
&MOTION
  &MD
    ENSEMBLE NVE
    STEPS  1
  &END
&END MOTION
&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME  HFX_BASIS
    BASIS_SET_FILE_NAME  BASIS_ADMM
    SORT_BASIS EXP
    POTENTIAL_FILE_NAME  POTENTIAL
    &AUXILIARY_DENSITY_MATRIX_METHOD
      ADMM_PURIFICATION_METHOD NONE
    &END
    &MGRID
      CUTOFF  100
      REL_CUTOFF  30
    &END MGRID
    &POISSON
      PERIODIC NONE
      POISSON_SOLVER MT
    &END POISSON
    &QS
      METHOD GPW
      EPS_DEFAULT 1.0E-11
    &END QS
    &SCF
      SCF_GUESS ATOMIC
      EPS_SCF 1.0E-6
      MAX_SCF 100
    &END SCF
    &XC
      # RPA@PBE0 with ADMM
      &XC_FUNCTIONAL
         &PBE
            SCALE_X 0.75
            SCALE_C 1.00
         &END
      &END XC_FUNCTIONAL 
      &HF
         FRACTION 0.25
         &SCREENING
           EPS_SCHWARZ 1.0E-7
         &END SCREENING
      &END HF
      &WF_CORRELATION
        &RI_RPA
          QUADRATURE_POINTS  2
          ADMM
          &HF
            FRACTION 1.0000000
            &SCREENING
              EPS_SCHWARZ 1.0E-7
            &END SCREENING
          &END HF
        &END
        &LOW_SCALING
          MEMORY_CUT 2
        &END
        &INTEGRALS
          ERI_METHOD OS
        &END INTEGRALS
        &RI
          &RI_METRIC
            POTENTIAL_TYPE IDENTITY
          &END
        &END RI
      &END
    &END XC
    UKS
    MULTIPLICITY  2
  &END DFT
  &SUBSYS
    &CELL
      ABC [angstrom]  5.000   5.000   5.000
      PERIODIC NONE
    &END CELL
    &KIND H
      BASIS_SET  DZVP-GTH
      BASIS_SET AUX_FIT cFIT3
      BASIS_SET RI_AUX  RI_DZVP-GTH
      POTENTIAL  GTH-HF-q1
    &END KIND
    &KIND C
      BASIS_SET  DZVP-GTH
      BASIS_SET AUX_FIT cFIT3
      BASIS_SET RI_AUX  RI_DZVP-GTH
      POTENTIAL  GTH-HF-q4
    &END KIND
    &TOPOLOGY
      COORD_FILE_NAME  CH3.xyz
      COORD_FILE_FORMAT xyz
      &CENTER_COORDINATES
      &END
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL
