&GLOBAL
  PROJECT test
  RUN_TYPE VIBRATIONAL_ANALYSIS
  PREFERRED_DIAG_LIBRARY SL
  PRINT_LEVEL medium
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &DFT
    &QS
      METHOD xTB
    &END QS
    &SCF
      SCF_GUESS MOPAC
      &OT
         PRECONDITIONER FULL_ALL
         MINIMIZER DIIS
      &END OT
      &OUTER_SCF
         MAX_SCF 10
         EPS_SCF 1.0E-8
      &END OUTER_SCF
      MAX_SCF 10
      EPS_SCF 1.0E-8
    &END SCF
  &END DFT
  &SUBSYS
    &CELL
      ABC [angstrom] 6.0 6.0 6.0
      PERIODIC NONE
    &END CELL
     &COORD
       UNIT angstrom
       O   0.000000    0.000000    0.000000
       H   0.000000   -0.757136    0.580545
       H   0.000000    0.757136    0.580545
     &END COORD
    &TOPOLOGY
     &CENTER_COORDINATES T
     &END
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL
&VIBRATIONAL_ANALYSIS
 &PRINT
  &NAMD_PRINT
  &END NAMD_PRINT
 &END PRINT
 DX 0.001
&END VIBRATIONAL_ANALYSIS
